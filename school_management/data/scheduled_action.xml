<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="ir_cron_course_reminder" model="ir.cron">
        <field name="name">Send Course Reminder Emails</field>
        <field name="model_id" ref="model_school_management_course_reminder"/>
        <field name="state">code</field>
        <field name="code">model.send_course_reminders()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="active" eval="True"/>
    </record>
</odoo>
